echo 'as3 interpreter'

fname=$1
content=''
  
exec<$fname
value=0
while read line
	do
		if [ $value != 0 ]
			then
				content+=$line;
				content+='\n';
		fi
		#echo $line
		value=`expr $value + 1`;
       #echo $value;
done
echo $content > ~/Projects/as3-interpreter/tmp/tmp.as
~/Projects/as3-interpreter/lib/redshell ~/Projects/as3-interpreter/tmp/tmp.as
#echo $content;
#echo "****$value";